{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/frontend/reportlabf/index.html"
        }
    ],
    "framework": "vite",
    "installCommand": "cd frontend/reportlabf && npm install",
    "buildCommand": "cd frontend/reportlabf && npm run build",
    "devCommand": "cd frontend/reportlabf && npx vite --port $PORT",
    "outputDirectory": "frontend/reportlabf/dist"
}